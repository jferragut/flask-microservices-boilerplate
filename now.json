{
    "name": "flask-microservices-framework",
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.py",
            "use": "@now/python"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "^(/.*)",
            "methods": ["GET","POST"],
            "dest": "api/**/$1.py", 
            "continue": true
        }
    ]
}